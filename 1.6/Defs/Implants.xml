<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
	<!-- Anima Eye -->
	<HediffDef ParentName="AddedBodyPartBase" Abstract="True" Name="TSOA_AnimaEyeBase">
		<description>A chunk of jade carved into the shape of an eye, infused with anima tree essence and animated by the user's psychic attunement. The eye helps the user perceive the underlying truth not just of the world, but of themself. This self-insight brings wisdom.</description>
		<descriptionHyperlinks>
			<ThingDef>TSOA_AnimaEye</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>TSOA_AnimaEye</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<betterThanNatural>true</betterThanNatural>
			<partEfficiency>0.5</partEfficiency>
		</addedPartProps>
		<renderNodeProperties>
			<li Class="PawnRenderNodeProperties_Eye">
				<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
				<texPath>Things/Pawn/Wounds/BionicEye</texPath>
				<parentTagDef>Head</parentTagDef>
				<drawSize>0.15</drawSize>
				<color>(20,90,20)</color>
				<drawData>
					<defaultData>
						<layer>56</layer>
						<offset>(0, 0, -0.25)</offset>
					</defaultData>
					<dataWest>
						<flip>true</flip>
					</dataWest>
				</drawData>
			</li>
		</renderNodeProperties>
		<tags>
			<li>TSOA_AnimaEye</li>
		</tags>
	</HediffDef>

	<HediffDef ParentName="TSOA_AnimaEyeBase">
		<defName>TSOA_AnimaEyeOne</defName>
		<label>anima eye (inert)</label>
		<description>A chunk of jade carved into the shape of an eye, infused with anima tree essence and animated by the user's psychic attunement. The user's psychic connection is too weak to animate it, and the eye is nearly blind.</description>
		<comps>
			<li Class="nuff.tsoa.core.HediffCompProperties_Evolving">
				<evolvingStat>PsychicSensitivity</evolvingStat>
				<upperThreshold>0.49</upperThreshold>
				<hediffAbove>TSOA_AnimaEyeTwo</hediffAbove>
			</li>
		</comps>
	</HediffDef>

	<HediffDef ParentName="TSOA_AnimaEyeBase">
		<defName>TSOA_AnimaEyeTwo</defName>
		<label>anima eye (dampened)</label>
		<description>A chunk of jade carved into the shape of an eye, infused with anima tree essence and animated by the user's psychic attunement. The eye is weakly attuned, and provides only a fraction of its potential.</description>
		<stages>
			<li>
				<statOffsets>
					<PawnTrapSpringChance>-0.5</PawnTrapSpringChance>
				</statOffsets>
				<statOffsetEffectMultiplier>PsychicSensitivity</statOffsetEffectMultiplier>
			</li>
		</stages>
		<comps>
			<li Class="nuff.tsoa.core.HediffCompProperties_Evolving">
				<evolvingStat>PsychicSensitivity</evolvingStat>
				<upperThreshold>0.89</upperThreshold>
				<hediffAbove>TSOA_AnimaEyeThree</hediffAbove>
				<lowerThreshold>0.5</lowerThreshold>
				<hediffBelow>TSOA_AnimaEyeOne</hediffBelow>
			</li>
		</comps>
	</HediffDef>

	<HediffDef ParentName="TSOA_AnimaEyeBase">
