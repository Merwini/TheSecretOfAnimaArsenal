	<ThingDef ParentName="BodyPartBionicBase">
		<defName>TSOA_AnimaEye</defName>
		<label>anima eye</label>
		<description>A chunk of jade carved into the shape of an eye, infused with anima tree essence and animated by the user's psychic attunement. Helps the user focus on their inner self, reflecting on their experience to gain wisdom. Can only install one.</description>
		<graphicData>
			<texPath>Things/Item/Health/HealthItem</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.9</drawSize>
			<color>(85,85,85)</color>
		</graphicData>
		<descriptionHyperlinks>
			<RecipeDef>TSOA_InstallAnimaEye</RecipeDef>
			<HediffDef>TSOA_AnimaEyeThree</HediffDef>
		</descriptionHyperlinks>
		<costList>
			<Plasteel>20</Plasteel>
			<ComponentSpacer>5</ComponentSpacer>
		</costList>
		<statBases>
			<Mass>8</Mass>
		</statBases>
		<thingSetMakerTags>
			<li>RewardStandardMidFreq</li>
		</thingSetMakerTags>
	</ThingDef>

	<RecipeDef>
		<defName>TSOA_InstallAnimaEye</defName>
		<label>install anima eye</label>
		<jobString>Installing anima eye.</jobString>
		<workAmount>2500</workAmount>
		<workerClass>Recipe_InstallImplant</workerClass>
		<appliedOnFixedBodyParts>
			<li>Eye</li>
		</appliedOnFixedBodyParts>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>TSOA_AnimaEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
